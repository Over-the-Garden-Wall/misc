	Number = {Pt 17},
	Pages = {3243-53},
	Pmc = {PMC2976019},
	Pmid = {20624793},
	Pst = {ppublish},
	Title = {Synaptic inputs and timing underlying the velocity tuning of direction-selective ganglion cells in rabbit retina},
	Volume = {588},
	Year = {2010},
