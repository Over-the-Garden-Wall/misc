	Month = {Dec},
	Number = {3},
	Pages = {847-56},
	Pmid = {11163271},
	Pst = {ppublish},
	Title = {Bipolar cells use kainate and AMPA receptors to filter visual information into separate channels},
	Volume = {28},
	Year = {2000}}

