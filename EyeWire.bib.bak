	Mesh = {Acetylcholine; Amacrine Cells; Animals; Computer Simulation; Dendrites; Humans; Nerve Net; Neural Networks (Computer); Receptors, Cholinergic; Receptors, GABA; Synapses; gamma-Aminobutyric Acid},
	Month = {Jul},
	Number = {1},
	Pages = {471-7},
	Pmid = {16598066},
	Pst = {ppublish},
	Title = {Symmetric interactions within a homogeneous starburst cell network can lead to robust asymmetries in dendrites of starburst amacrine cells},
	Volume = {96},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1152/jn.00628.2005}}

