	Mesh = {Action Potentials; Animals; Chlorides; In Vitro Techniques; Motion Perception; Neural Conduction; Neural Inhibition; Patch-Clamp Techniques; Rabbits; Retina; Retinal Ganglion Cells; Synapses; Synaptic Transmission; Vision, Ocular},
	Month = {Sep},
	Number = {17},
	Pages = {7712-20},
