	Journal = {Neuron},
	Journal-Full = {Neuron},
	Mesh = {Acetylcholine; Animals; Cell Communication; Electrophysiology; Mice; Mice, Transgenic; Motion Perception; Nystagmus, Optokinetic; Reflex, Pupillary; Retinal Ganglion Cells; Vision, Ocular; gamma-Aminobutyric Acid},
	Month = {Jun},
	Number = {3},
	Pages = {771-80},
	Pmid = {11430810},
	Pst = {ppublish},
	Title = {A key role of starburst amacrine cells in originating retinal directional selectivity and optokinetic eye movement},
	Volume = {30},
	Year = {2001}}

