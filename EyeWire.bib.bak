	Mesh = {Amacrine Cells; Animals; Calcium Signaling; Dendrites; Mice; Mice, Inbred C57BL; Microscopy, Electron; Microscopy, Fluorescence; Models, Neurological; Neural Pathways; Neuroanatomical Tract-Tracing Techniques; Retina; Retinal Ganglion Cells; Synapses},
	Month = {Mar},
	Number = {7337},
	Pages = {183-8},
	Pmid = {21390125},
	Pst = {ppublish},
	Title = {Wiring specificity in the direction-selectivity circuit of the retina},
	Volume = {471},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature09818}}

