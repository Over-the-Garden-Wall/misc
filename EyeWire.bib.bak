	Journal-Full = {Neuron},
	Mesh = {Action Potentials; Animals; Calcium; Color Vision; Dose-Response Relationship, Radiation; Functional Laterality; GABA Antagonists; Green Fluorescent Proteins; Mice; Mice, Inbred C57BL; Mice, Transgenic; Opsins; Phosphinic Acids; Photic Stimulation; Pyridazines; Pyridines; Retina; Retinal Cone Photoreceptor Cells; Retinal Ganglion Cells; Sodium Channel Blockers; Tetrodotoxin; Visual Pathways; Visual Perception},
	Month = {Feb},
